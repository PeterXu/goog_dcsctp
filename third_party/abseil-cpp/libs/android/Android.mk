LOCAL_PATH:= $(call my-dir)
$(info PLATFORM is '$(TARGET_ARCH)' TARGET_ARCH_ABI is '$(TARGET_ARCH_ABI)')
include $(CLEAR_VARS)

ABSL_HOME_PATH  := $(LOCAL_PATH)/../..

LOCAL_SRC_FILES := $(TARGET_ARCH_ABI)/libabsl_all.a

LOCAL_C_INCLUDES += $(ABSL_HOME_PATH)

LOCAL_EXPORT_C_INCLUDES := $(ABSL_HOME_PATH)

LOCAL_MODULE := libabsl_all
LOCAL_MODULE_TAGS := optional

include $(PREBUILT_STATIC_LIBRARY)
